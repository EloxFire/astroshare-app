      }
    },
    "production": {
      "env": {
        "EXPO_PUBLIC_ASTROSHARE_API_URL":"https://api.astroshare.fr",
        "EXPO_PUBLIC_APP_VERSION":"v1.0.0",
        "EXPO_PUBLIC_LAST_UPDATE":"2024-07-28"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "track": "internal"

      }
    }
  }
}
